typestate TravelAgencyProtocol {

	State0 = {
		String getInfo(): State0,
		void receiveRequest(String): State1
	}
	
	State1 = {
		PaymentStatus tryPayment(String): <OK: State2, KO: State3, TA: State1>
	}
	
	State2 = {
		String getInvoice(): State3
	}
	
	State3 = {
		OurBool exit(OurBool): <TRUE: end, FALSE: State0>
	}

}
