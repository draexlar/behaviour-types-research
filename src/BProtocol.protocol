package Agency;

typestate BProtocol {
	State0 = {
		int receive_checkPriceFromC(): State1
	}
	State1 = {
		void send_APPROVEToCA(): State2,
		void send_REFUSEToCA(): State4
	}
	State2 = {
		void send_approveCodeToCA(String): State3
	}
	State3 = {
		void send_paymentPriceToA(int): end
	}
	State4 = {
		void send_refuseCodeToAC(String): end
	}
}
