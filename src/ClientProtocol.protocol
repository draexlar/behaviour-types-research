typestate ClientProtocol {

	State0 = {
		String receiveInfo(): State0,
		void sendRequest(String): State1
	}
	
	State1 = {
		PaymentStatus tryPayment(String): <OK: State2, KO: State3, TA: State1>
	}
	
	State2 = {
		String receiveInvoice(): State3
	}
	
	State3 = {
		OurBool exit(OurBool): <TRUE: end, FALSE: State0>
	}

}
